{
    "name": "laravelir/pwa",
    "description": "pwa for laravel",
    "homepage": "https://github.com/laravelir/pwa",
    "type": "library",
    "version": "0.1.6",
    "keywords": [
        "laravel",
        "laravel-package",
        "laravel support",
        "lumen packages",
        "laravel packages",
        "lumen support",
        "pwa",
        "laravel pwa"
    ],
    "authors": [
        {
            "name": "miladimos",
            "email": "miladimos@outlook.com",
            "role": "maintainer",
            "homepage": "https://github.com/miladimos"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laravelir\\Pwa\\": "src/"
        },
        "files": [
            "./src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Laravelir\\Pwa\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravelir\\Pwa\\Providers\\PwaServiceProvider"
            ],
            "aliases": {
                "Pwa": "Laravelir\\Pwa\\Facades\\Pwa"
            }
        }
    },
    "require": {
        "php": ">=7.4|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "minimum-stability": "stable",
    "license": "MIT"
}
